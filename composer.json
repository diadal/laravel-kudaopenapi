{
  "name": "diadal/laravel-kudaopenapi",
  "description": "KudaBank Open APi for virtual account",
  "keywords": [
    "virtual account",
    "bank",
    "kuda",
    "kudaopenapi"
  ],
  "homepage": "https://github.com/diadal/laravel-kudaopenapi",
  "authors": [
    {
      "name": "Diadal Nigeria Ltd",
      "email": "info@diadal.com.ng",
      "homepage": "https://diadal.com.ng"
    }
  ],
  "license": "MIT",
  "require": {
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^6.0.4|^7.0|^8.0|^9.0",
    "phpseclib/phpseclib": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Diadal\\Kuda\\": "src"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.1"
    },
    "laravel": {
      "providers": [
        "Diadal\\Kuda\\KudaServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
